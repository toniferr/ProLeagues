<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="JORNADA 4"
            android:id="@+id/textView11" android:layout_gravity="center_horizontal" android:textSize="50dp"/>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="207dp" android:layout_marginTop="50dp">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_gravity="center_horizontal">
            <EditText
                    android:layout_width="44dp"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:ems="10"
                    android:id="@+id/editText" android:layout_weight="0.74"/>
            <TextView
                    android:layout_width="120dp"
                    android:layout_height="match_parent"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:id="@+id/t1"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="vs"
                    android:id="@+id/textView3" android:layout_weight="0.2"/>
            <TextView
                    android:layout_width="120dp"
                    android:layout_height="match_parent"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:id="@+id/textView4"/>
            <EditText
                    android:layout_width="44dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/editText2" android:layout_weight="0.74"/>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:layout_width="wrap_content"
                      android:layout_height="wrap_content" android:layout_gravity="center_horizontal">
            <EditText android:layout_width="44dp" android:layout_height="wrap_content" android:inputType="number"
                      android:ems="10" android:id="@+id/editText3" android:layout_weight="0.74"/>
            <TextView android:layout_width="120dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge"
                      android:id="@+id/textView5" android:layout_weight="1"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceSmall" android:text="vs"
                      android:id="@+id/textView6" android:layout_weight="0.2"/>
            <TextView android:layout_width="120dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge"
                      android:id="@+id/textView7" android:layout_weight="1"/>
            <EditText android:layout_width="44dp" android:layout_height="wrap_content" android:id="@+id/editText4"
                      android:layout_weight="0.74"/>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:layout_width="wrap_content"
                      android:layout_height="wrap_content" android:layout_gravity="center_horizontal">
            <EditText android:layout_width="44dp" android:layout_height="wrap_content" android:inputType="number"
                      android:ems="10" android:id="@+id/editText5" android:layout_weight="0.74"/>
            <TextView android:layout_width="120dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge"
                      android:id="@+id/textView8" android:layout_weight="1"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceSmall" android:text="vs"
                      android:id="@+id/textView9" android:layout_weight="0.2"/>
            <TextView android:layout_width="120dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge"
                      android:id="@+id/textView10" android:layout_weight="1"/>
            <EditText android:layout_width="44dp" android:layout_height="wrap_content" android:id="@+id/editText6"
                      android:layout_weight="0.74"/>
        </LinearLayout>
    </LinearLayout>
    <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enviar jornada"
            android:id="@+id/button" android:layout_gravity="center_horizontal"/>
</LinearLayout>